{
  "name": "dapp-starter",
  "version": "0.4.0",
  "scripts": {
    "build": "npx hardhat compile --network localhost && next build",
    "clean": "npx hardhat clean && rimraf -rf src/types",
    "dev": "next dev",
    "lint": "next lint",
    "lint:sol": "npx solhint 'contracts/**/*.sol'",
    "prepare": "husky install",
    "start": "next start",
    "test": "npx hardhat test"
  },
  "nano-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --quiet --fix",
      "prettier --write",
      "rustywind --write"
    ],
    "*.{css,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@ethersproject/providers": "5.7.2",
    "@ethersproject/units": "5.7.0",
    "@tailwindcss/typography": "0.5.10",
    "@web3-react/core": "6.1.9",
    "@web3-react/injected-connector": "6.0.7",
    "@web3-react/network-connector": "6.2.9",
    "@web3-react/walletconnect-connector": "6.2.13",
    "daisyui": "4.0.3",
    "loglevel": "1.8.1",
    "next": "14.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "web3-react": "5.0.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "1.0.9",
    "@nomicfoundation/hardhat-toolbox": "2.0.2",
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.1.7",
    "@openzeppelin/contracts": "5.0.0",
    "@openzeppelin/contracts-upgradeable": "5.0.0",
    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@typechain/ethers-v5": "11.1.2",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "9.1.0",
    "@types/chai": "4.3.10",
    "@types/mocha": "10.0.4",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "autoprefixer": "10.4.16",
    "chai": "4.3.10",
    "dotenv": "16.3.1",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-productsway": "1.3.0",
    "ethers": "5.7.2",
    "hardhat": "2.19.0",
    "hardhat-gas-reporter": "1.0.9",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "next-secure-headers": "2.2.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-solidity": "1.2.0",
    "rimraf": "5.0.5",
    "rustywind": "0.20.0",
    "solgraph": "1.0.2",
    "solhint": "4.0.0",
    "solidity-coverage": "0.8.5",
    "sort-package-json": "2.6.0",
    "tailwindcss": "3.3.5",
    "ts-node": "10.9.1",
    "typechain": "8.3.2",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.1"
}
