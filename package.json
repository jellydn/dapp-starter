{
  "name": "dapp-starter",
  "version": "0.2.0",
  "scripts": {
    "build": "npx hardhat compile --network localhost && next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:sol": "npx solhint 'contracts/**/*.sol'",
    "prepare": "husky install",
    "start": "next start",
    "test": "npx hardhat test"
